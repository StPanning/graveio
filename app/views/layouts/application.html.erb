
<!DOCTYPE html>
<html>
<head>
  <title>graveio</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>

<body>
    <!-- Main navigation fixed on top -->
    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
            <a class="brand" href="<%= root_url%>"> graveio </a>
            <ul class="nav">
              <li class="active">
              </li>
            </ul>
            <ul class="nav pull-right">
              <li>
                <%= link_to 'new snippet', new_post_path %>
              </li>
            </ul>
        </div>
      </div>
    </div>
    <div class="container">
      <%= yield %>
    </div>
</body>
</html>
